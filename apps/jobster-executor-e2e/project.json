{
  "name": "jobster-executor-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["jobster-executor"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/jobster-executor-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["jobster-executor:build", "jobster-executor:serve"]
    }
  }
}
